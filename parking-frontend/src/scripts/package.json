{
  "name": "parksathi-data-scraper",
  "version": "1.0.0",
  "description": "Galli Maps parking data scraper and database import tool for ParkSathi",
  "type": "module",
  "main": "syncParkingData.js",
  "scripts": {
    "start": "node syncParkingData.js",
    "scrape": "node syncParkingData.js",
    "test-scrape": "node syncParkingData.js --dry-run",
    "validate-only": "node syncParkingData.js --skip-import",
    "scrape-only": "node syncParkingData.js --skip-validation --skip-import"
  },
  "bin": {
    "sync-parking-data": "./syncParkingData.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.0",
    "mongodb": "^6.2.0",
    "ora": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  },
  "keywords": [
    "parking",
    "galli-maps",
    "nepal",
    "kathmandu",
    "scraper",
    "mongodb",
    "parksathi"
  ],
  "author": "ParkSathi Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/parksathi.git"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
